<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form id="gst-product-company-form" title="Product Company" name="product-company-form"
    model="com.axelor.apps.base.db.ProductCompany" extension="true">
    <extend target="//field[@name='managPriceCoef']">
      <insert position="after">
        <field name="gstRate" colSpan="6" if-module="axelor-gst"/>
      </insert>
      <attribute name="colSpan" value="6"/>
    </extend>
  </form>

</object-views>
