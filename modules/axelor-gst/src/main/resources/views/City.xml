<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form id="gst-city-form" name="city-form" title="City" model="com.axelor.apps.base.db.City"
    extension="true">
    <extend target="//field[@name='country']">
      <insert position="after">
        <field if-module="axelor-gst" name="state" form-view="state-form" grid-view="state-grid" if="__config__.app.isApp('gst')" canSelect="true"/>
      </insert>
      
      
    </extend>
  </form>
</object-views>