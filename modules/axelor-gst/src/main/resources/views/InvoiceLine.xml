<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	<form id="gst-invoice-line-form" name="invoice-line-form" title="InvoiceLine" 
		extension="true" model="com.axelor.apps.account.db.InvoiceLine" >
		
		<extend target="//field[@name='exTaxTotal']" >
			<insert position="before" >				
					<field name="gstRate" />
					<field name="igst" />
					<field name="sgst" /> 
					<field name="cgst" />
			</insert>
			
		</extend>
		
		<extend target="//field[@name='productName']">
			<insert position="after" >
				<field name="hsbn" />
			</insert>
		</extend>
	</form>
</object-views>